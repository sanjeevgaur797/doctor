<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">
<script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ho+j7jyWK8fNQe+A12Hb8AhRq26LrZ/JpcUGGOn+Y7RsweNrtN/tE3MoK7ZeZDyx" crossorigin="anonymous"></script>
<style>
    .main{
        display: flex;
        justify-content: center;
        

    }
    .subdiv{
        background-color: rgb(238, 204, 14);
        border: 1px solid black;
        border-radius: 17px;
        margin-top: 20px;
        width: auto;
        height: auto;
        padding: 30px;
    }
    h1{
background-color: rgb(219, 224, 212);
text-align: center;
    color: coral;
    border:2px solid darkcyan;
border-top-left-radius: 50px;
border-bottom-right-radius: 50px;

    }
</style>
<html>
    <div  class='main'>
    <div class='subdiv'>
<table bordercolor='green' border='1px'>
  <h1><b><i>List of Doctors</i></b></h1>
    <tr>
        <th> Id</th>
        <th>Name</th>
        <th>DOB<br>Gender</th>
        <th>State</th>
        <th>City</th>
        <th>Zip Code</th>
        <th>Address</th>
        <th>mobile no</th>
        <th>picture</th>
        <th>Edit/Delete</th>
    </tr>
<% data.map(function(item,key){   %>
<tr>
  <td><%=item.doctorid%></td>
  <td><%=item.name%></td>
  <td><%=item.dob%><br><%=item.gender%></td>
  <td><%=item.statename%></td>
  <td><%=item.cityname%></td>
  <td><%=item.zip%></td>
  <td><%=item.address%></td>
  <td><%=item.number1%><br><%=item.number2%></td>
  <td><a href='/doctor/editpicture?did=<%=item.doctorid%>'><img src='/images/<%=item.picture%>' width=60px height=60px></a></td>

  
  <td><a href='/doctor/doctorbyid?did=<%=item.doctorid%>'>Edit/Delete</a></td>
</tr>
    
<% }) %>
</table>

</div>
</div>
</html>